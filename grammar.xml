=======================================================================================================
= Фразовые предложения                                                                                         =
=======================================================================================================
-->
<!-- Именные Группы -->

<PhraseCategory>
  <TestPhrase>карта</TestPhrase>
  <Pattern>
    <Element><WordForm></WordForm><Category>С</Category><Grammems>$Gender,$Number,$Case</Grammems></Element>
  </Pattern>
  <TrasformationFormula>#1l</TrasformationFormula>
  <Def><Category>Иг</Category><Grammems>$Gender,3л,$Number,$Case</Grammems></Def>
  <Vars>
    <Var Name="$Number" Values="ед,мн"  />
    <Var Name="$Gender" Values="мр,ср,жр" />
    <Var Name="$Case" Values="им,вн,рд" />
  </Vars>
</PhraseCategory>

